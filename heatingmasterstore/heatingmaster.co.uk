Listen 9000
<VirtualHost *>
    ServerName www.heatingmaster.co.uk
    CustomLog /var/log/apache2/access.log combined
    ErrorLog /var/log/apache2/error.log
    AllowEncodedSlashes On

    WSGIDaemonProcess heatingmaster processes=2 threads=5 display-name=heatingmaster maximum-requests=10000
    WSGIProcessGroup heatingmaster
    WSGIApplicationGroup %{GLOBAL}
    WSGIScriptAlias / /home/shu/www/heatingmasterstore/heatingmasterstore/heatingmaster.wsgi
    WSGIPassAuthorization On

    Alias /media/  /home/shu/www/heatingmasterstore/heatingmasterstore/public/media/
    Alias /static/ /home/shu/www/heatingmasterstore/heatingmasterstore/public/static/
    
    <Directory /home/shu/www/heatingmasterstore/heatingmasterstore>
        Order deny,allow
        Allow from all
    </Directory>

</Virtualhost>